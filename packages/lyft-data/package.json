{
  "name": "@dynamic-data/lyft-data",
  "version": "1.0.0",
  "description": "Dynamic Data Faker for Lyft",
  "author": "Jashaul <djashaul@gmail.com>",
  "homepage": "https://github.com/Jashaul/dynamic-data#readme",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": "./dist/esm/index.js",
    "./models": {
      "import": "./dist/esm/models/index.js",
      "require": "./dist/cjs/models/index.js"
    },
    "./models/*": {
      "import": "./dist/esm/models/*.js",
      "require": "./dist/cjs/models/*.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rm -rf ./dist && rollup -c",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.12",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/runtime": "^7.17.9",
    "babel-jest": "^27.5.1",
    "cross-env": "^7.0.3",
    "jest": "^27.5.1"
  }
}
